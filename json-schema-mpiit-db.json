{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "",
  "type": "object",
  "properties": {
    "jobs": {
      "type": "array",
      "uniqueItems": true,
      "minItems": 1,
      "items": {
        "required": [
          "job_name",
          "job_short_name",
          "build_id",
          "state"
        ],
        "properties": {
          "job_name": {
            "type": "string",
            "minLength": 1
          },
          "job_short_name": {
            "type": "string",
            "minLength": 1
          },
          "build_id": {
            "type": "string",
            "minLength": 1
          },
          "state": {
            "type": "string",
            "minLength": 1
          },
          "urls": {
            "type": "object",
            "properties": {
              "dashboard": {
                "type": "string",
                "minLength": 1
              },
              "firewatch_build_log": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": [
              "dashboard",
              "firewatch_build_log"
            ]
          },
          "firewatch_logs": {
            "type": "array",
            "uniqueItems": true,
            "minItems": 1,
            "items": {
              "required": [
                "datetime",
                "src",
                "level",
                "message"
              ],
              "properties": {
                "datetime": {
                  "type": "string",
                  "minLength": 1
                },
                "src": {
                  "type": "string",
                  "minLength": 1
                },
                "level": {
                  "type": "string",
                  "minLength": 1
                },
                "message": {
                  "type": "string",
                  "minLength": 1
                }
              }
            }
          }
        }
      }
    }
  },
  "required": [
    "jobs"
  ]
}